{
  "name": "jsstore_typescript_example",
  "version": "1.0.0",
  "scripts": {
    "install:lib": "node installer.js",
    "install:build": "npm run install:lib && npm run build",
    "build:typescript": "webpack --config typescript/webpack.config.js",
    "dev": "webpack-dev-server --config typescript/webpack.config.js",
    "dev:todo": "webpack-dev-server --config todo/webpack.config.js",
    "install:dev": " npm run install:lib && npm run dev",
    "install:test": " npm run install:lib && npm run test",
    "start": "npm run dev",
    "test": "cd typescript && npm run test:browser",
    "test:mochapack": "cross-env NODE_ENV=test mochapack --require ./typescript/test/setup.js --webpack-config typescript/webpack.config.test.js \"typescript/test/**/*.test.ts\""
  },
  "dependencies": {
    "chai": "^4.3.4",
    "flexboot": "^1.0.1",
    "jquery": "^3.6.0",
    "karma-typescript": "^5.5.1",
    "mahal": "file:../mahal-0.21.0.tgz",
    "mahal-html-compiler": "file:../../mahal-html-compiler/mahal-html-compiler-0.18.0.tgz",
    "mahal-test-utils": "file:../../mahal-test-utils/mahal-test-utils-0.4.0.tgz",
    "mocha": "^8.3.2",
    "sinon": "^9.2.4"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.2",
    "@types/sinon": "^9.0.11",
    "cross-env": "^7.0.3",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "karma": "^6.3.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^4.0.2",
    "puppeteer": "^11.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.1.0",
    "typescript": "^3.9.9",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-node-externals": "^2.5.2"
  }
}
